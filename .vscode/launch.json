  "version": "0.2.0",
  "configurations": [
    {
      "name": "llm-api",
      "type": "python",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/app",
